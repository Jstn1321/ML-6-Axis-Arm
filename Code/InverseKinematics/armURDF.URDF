<robot name = "JN_V1">

	<link name = "base">
		<visual>
			<origin xyz = "-69.87 0 20" rpy = "0 0 0"/>
			<geometry>
				<box size = "279.75 140 40" rpy = "0 0 0"/>
			</geometry>
		</visual>
	</link>

	<link name = "base_link">
		<visual>
			<origin xyz = "0 0 74" rpy = "0 0 0"/>
			<geometry>
				<cylinder length = "68" radius = "50.22"/>
			</geometry>
		</visual>
	</link>

	<joint name="fixedBase" type="fixed">
    	<parent link="base"/>
    	<child link="base_link"/>
   		<origin xyz = "0 0 0" rpy = "0 0 0"/>
  	</joint>

	<link name = "shoulder_mount">
		<visual>
          <origin xyz = "35.29 0 40" rpy = "0 0 0"/>
			<geometry>
				<box size = "171.02 100.44 88"/>
			</geometry>
		</visual>
	</link>

	<joint name = "base_rot" type = "revolute">
		<parent link = "base_link"/>
		<child link = "shoulder_mount"/>
		<origin xyz = "0 0 112" rpy = "0 0 0"/>
		<axis xyz = "0 0 -1"/>
	</joint>

	<link name = "humorus">
		<visual>
          <origin xyz = "0 11 100" rpy = "0 0 0"/>
			<geometry>
				<box size = "65.54 22 265.54"/>
			</geometry>
		</visual>
	</link>

	<joint name = "shoulder_rot" type = "revolute">
		<parent link = "shoulder_mount"/>
		<child link = "humorus"/>
		<origin xyz = "75.15 50.22 40" rpy = "0 1.57 0"/>
		<axis xyz = "0 -1 0"/>
      <limit lower = "0" upper = "3.14"/>
	</joint>

	<link name = "extrudeHumorus">
		<visual>
          <origin xyz = "0 0 0" rpy = "1.57 0 0"/>
			<geometry>
				<cylinder length = "13" radius = "32.767"/>
			</geometry>
		</visual>
	</link>

	<joint name="fixedHumorus" type="fixed">
    	<parent link="humorus"/>
    	<child link="extrudeHumorus"/>
   		<origin xyz = "0 -6.5 200" rpy = "0 0 0"/>
  	</joint>

	<link name = "elbow">
		<visual>
          <origin xyz = "0 0 24.5" rpy = "0 0 0"/>
			<geometry>
				<box size = "56 25 105"/>
			</geometry>
		</visual>
	</link>

	<joint name = "elbow_rot" type = "revolute">
		<parent link = "extrudeHumorus"/>
		<child link = "elbow"/>
		<origin xyz = "0 -19 0" rpy = "0 0 0"/>
		<axis xyz = "0 -1 0"/>
	</joint>

	<link name = "radiulna">
		<visual>
          <origin xyz = "0 0 54.665" rpy = "0 0 0"/>
			<geometry>
				<box size = "64 55 109.33"/>
			</geometry>
		</visual>
	</link>

	<joint name = "elbow_twist" type = "revolute">
		<parent link = "elbow"/>
		<child link = "radiulna"/>
		<origin xyz = "0 0 77" rpy = "0 0 0"/>
		<axis xyz = "0 0 -1"/>
	</joint>

	<link name = "wrist">
		<visual>
          <origin xyz = "0 0 29.75" rpy = "0 0 0"/>
			<geometry>
				<box size = "55 56 59.5"/>
			</geometry>
		</visual>
	</link>

	<joint name = "wrist_rot" type = "revolute">
		<parent link = "radiulna"/>
		<child link = "wrist"/>
		<origin xyz = "0 0 109.33" rpy = "0 0 0"/>
		<axis xyz = "0 -1 0"/>
	</joint>

	<link name = "gripper">
		<visual>
          <origin xyz = "0 0 119" rpy = "0 0 0"/>
			<geometry>
				<box size = "98 55 119"/>
			</geometry>
		</visual>
	</link>

	<joint name = "wrist_twist" type = "revolute">
		<parent link = "wrist"/>
		<child link = "gripper"/>
		<origin xyz = "0 0 0" rpy = "0 0 0"/>
		<axis xyz = "0 0 -1"/>
	</joint>

</robot>